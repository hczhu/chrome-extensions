{
  "name": "Process numbers on the page",
  "description": "Select numbers and process.",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["<all_urls>", "tabs"],
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2,
  "content_scripts": [{
        "matches": ["*://*/*"],
        "all_frames": true,
        "js": ["process_selection.js"],
        "run_at": "document_end"
    }],
  "permissions" : ["tabs"]
}
